<!DOCTYPE html>
<html>
  <head>
    <title>PsychoWarfareWebsite</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <% if Rails.env.production? %>
    <script type="text/javascript">
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-XXXXXXXX-X', 'auto');
    </script>
    <% end %>
  </head>

  <body>


 <div class="ui fixed inverted menu">
    <div class="ui container">
    <%= link_to(root_path , class: "header item") do %>
        <%= image_tag("logo80x80.png", class: "logo") %> Psycho Warfare
    <% end %>
      <%= link_to("About", home_contact_path , class: "item") %> 
    </div>
  </div>

    <%= render 'masthead' %>

   <div class="ui column center aligned container celled grid vertical stripe segment">
  
     <div class="ui twelve wide column">

       <%= yield %>

     </div>

        <div class="ui four wide column">
          
          <%= render 'sidebar' %>

        </div>
   </div>
    
    <%= render 'emailModal' %>
    <%= render 'footer' %>
    <%= render 'messages' %>
  </body>
</html>
